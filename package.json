  "devDependencies": {
    "@babel/register": "^7.0.0",
    "assert": "^1.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "css-loader": "^2.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
