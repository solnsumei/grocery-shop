{
  "name": "grocery-shop",
  "version": "1.0.0",
  "description": "Simple grocery shop app",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean-dist && webpack -p && cp ./client/src/index.html ./dist",
    "clean-dist": "node_modules/.bin/rimraf ./dist && mkdir dist",
    "start:dev": "nodemon --exec babel-node server/devServer.js",
    "start": "babel-node server/server.js",
    "test:client": "jest",
    "test": "NODE_ENV=test nyc mocha --exit ./server/test/* --require @babel/register --timeout 10000",
    "heroku-postbuild": "yarn build"
  },
  "author": "Solomon Nsumei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solnsumei/grocery-shop.git"
  },
